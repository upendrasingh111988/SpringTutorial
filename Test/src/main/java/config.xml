<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:context="http://www.springframework.org/schema/context"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


<bean class="com.test.Test.Student" name="student1" p:studentId="1234" p:studentName="Upendra Singh" p:studentAddress="pune" />

<bean class="com.test.Test.Student" name="student2">
	
		<property name="studentId" value="12345"/>
		
		<property name="studentName" value="Vashu Singh"/>
		
		<property name="studentAddress" value="Patna"/>
		
	</bean>






</beans>